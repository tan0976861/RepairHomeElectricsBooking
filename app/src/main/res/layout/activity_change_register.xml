<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#0D11c7e6"
    tools:context=".ChangeRegisterActivity">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:layout_marginTop="65dp"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="15dp"
        android:orientation="vertical"
        android:background="@drawable/custom_layout_register">

        <Button
            android:id="@+id/btn_goto_signup_user"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Đăng ký User"/>
        <Button
            android:id="@+id/btn_goto_signup_worker"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Đăng ký Woker"/>

    </LinearLayout>

</LinearLayout>